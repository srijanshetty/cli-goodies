function _rclone_rmdirs {
  _arguments \
    '--leave-root[Do not remove root directory if empty]' \
    '--acd-auth-url[Auth server URL.]:' \
    '--acd-client-id[OAuth Client Id]:' \
    '--acd-client-secret[OAuth Client Secret]:' \
